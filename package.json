{
  "name": "dynamoflow",
  "version": "1.4.2",
  "description": "A practical & extendable DynamoDB client for TypeScript applications.",
  "license": "MIT",
  "keywords": [
    "dynamodb",
    "dynamo",
    "database",
    "serverless",
    "orm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jyelewis/DynamoFlow.git"
  },
  "type": "module",
  "author": "Jye Lewis <jye@jyelewis.com>",
  "main": "dist/index.js",
  "scripts": {
    "check:ts": "tsc --noEmit",
    "prettier": "prettier --check src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "eslint": "eslint src",
    "build": "tsc",
    "start": "ts-node --esm",
    "test": "jest",
    "check": "pnpm run check:ts && pnpm run prettier && pnpm run eslint && pnpm run test"
  },
  "files": [
    "docs",
    "dist",
    "!**/*.test.d.ts",
    "!**/*.test.js"
  ],
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.388.0",
    "@aws-sdk/lib-dynamodb": "^3.388.0",
    "@swc/core": "^1.3.76",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.10",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "ulid": "^2.3.0",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.359.0",
    "@aws-sdk/lib-dynamodb": "^3.359.0"
  }
}
